{
    "compileOnSave": false,
    "compilerOptions": {
      "module": "commonjs",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "./__out",
      "baseUrl": "./",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es5",
      "alwaysStrict": false,
      "noImplicitUseStrict": true,
      "typeRoots": [
      ],
      "types": [],
      "lib": [
        "es2018",
      ]
    },
    "include": [
      "babyloncli.ts",
      "canvas.ts",
      "window.ts",
      "simple_test.ts",
      "declarations.d.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
